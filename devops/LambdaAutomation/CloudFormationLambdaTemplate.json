{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This template is for creating lambda function",
    "Resources": {
        "LambdaFunResource": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "FunctionName": "Lambda-Function",
                
                "Code": {
                    "ZipFile": {
                        "Fn::Join": [
                            "\n",
                            [
                                
                                "def index.handler(event, context):",
                                " print('THIS IS LAMBDA CREATED FROM CloudFormation template')",
                                " return 'success'"
                            ]
                        ]
                    }
                },
                "Description": "Automaticaally created by the script",
                "Handler": "index.handler",
                "Role": "arn:aws:iam::061431082068:role/Lambda_Role",
                "Runtime": "nodejs14.x",
                "Timeout": 120
            }
        }
  }
}
